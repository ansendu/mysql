    "keywords": [
        "dazzle", "dazzle-php", "async", "sql", "mysql", "storage", "database"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dazzle-php/mysql/issues",
        "source": "https://github.com/dazzle-php/mysql"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/mysql/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7"
    },
    "require-dev": {
